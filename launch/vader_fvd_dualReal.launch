<launch>
    <!-- TF -->
    <node pkg="vader_planner" type="cam_transform_publisher.py" name="tform" output="screen"/>
    
    <!-- HRI state machine node -->
    <node pkg="vader_hri" type="FVD_Dual_HRI" name="FVD_Dual_HRI" output="screen">
    </node>

    <!-- Planner (context + VADER planner) + Gazebo launch -->
    <include file="$(find vader_planner)/launch/vader_dual_real.launch"> 

        <arg name="robot_ip_1" value="192.168.1.241"/> 
        <arg name="robot_ip_2" value="192.168.1.230"/>
    </include>

    <!-- end effectors -->
    <node pkg="vader_dynamixel" type="dynamixel_node.py" name="dynamixel" output="screen"/>

    <include file="$(find vader_perception)/launch/gripper_cam_pose_estimation.launch"></include>
</launch>